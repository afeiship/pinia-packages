{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "pretty": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ]
  },
  "include": [
    "src"
  ]
}
